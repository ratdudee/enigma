# Enigma machine key configuration
# Reflector: 1=B, 2=C
reflector=1

# Rotor selection: 1-8 (separated by commas)
wheels=1,2,3

# Ring settings (A-Z, separated by commas, must match number of wheels)
rings=A,B,C

# Plugboard connections (pairs separated by commas, e.g., AB,CD,EF)
plugboard=AR,BG,CX,DZ,EJ,FM,HN,IK,LO,PQ
